<p id="notice"><%= notice %></p>

<h1>Card Associations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @card_associations.each do |card_association| %>
      <tr>
        <td><%= card_association.name %></td>
        <td><%= link_to 'Show', card_association %></td>
        <td><%= link_to 'Edit', edit_card_association_path(card_association) %></td>
        <td><%= link_to 'Destroy', card_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Card Association', new_card_association_path %>
